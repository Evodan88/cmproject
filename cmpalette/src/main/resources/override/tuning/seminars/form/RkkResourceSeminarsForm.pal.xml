<?xml version='1.0' encoding='UTF-8'?>
<ui xmlns="http://www.intertrust.ru/schema/palette/tn-ui">
  <form schema-ref="RkkResourceSeminars" id="RkkResourceSeminarsForm" projection="default" showtabs="true">
    <appearance widget="tabs"/>
    <component>
      <!-- Шапка -->
      <section id="Header">
        <appearance field-layout="horizontal"/>
        <component>
          <field attribute-ref="showIdent">
            <properties>
              <read-only condition-union="AND">
                <condition>true</condition>
              </read-only>
            </properties>
            <appearance align="left" importance="normal">
              <label-style field-width="4" field-unit="symbol"/>
              <alt-appearancies>
                <alt-appearance>
                  <appearance-ref ref="css_headerElementWithIcon"/>
                  <enable condition-union="AND">
                    <condition>true</condition>
                  </enable>
                </alt-appearance>
              </alt-appearancies>
            </appearance>
          </field>
        </component>
      </section>
      <!-- Тело -->
      <section id="Attributes" name="${ui.form.RkkResourceSeminars.section.Attributes.name:Описание}">
        <component>
          <field attribute-ref="nameSem" name="${ui.form.RkkResourceSeminars.Attributes.author.section:Название:}"/>
          <field attribute-ref="author" name="${ui.form.RkkResourceSeminars.Attributes.author.name:Ведущий:}"/>
          <field attribute-ref="participants" name="${ui.form.RkkResourceSeminars.Attributes.executor.name:Участники:}"/>
          <field attribute-ref="place" name="${ui.form.RkkResourceSeminars.Attributes.author.section:Место проведения:}"/>
          <field attribute-ref="time" name="${ui.form.RkkResourceSeminars.Attributes.author.section:Дата/Время проведения:}"/>
          <field attribute-ref="materials" name="${ui.form.RkkResourceSeminars.Attributes.author.section:Материалы семинара:}"/>
        </component>
      </section>
      <section id="Attributes" name="${ui.form.RkkResourceSeminars.section.Attributes.name:Материалы}">
        <component>

        </component>
      </section>

    </component>
  </form>
</ui>